{
  "name": "cost-katana-ai-optimizer",
  "displayName": "Cost Katana AI Optimizer",
  "description": "AI-powered cost optimization for Cursor/VS Code - Track, analyze, and optimize AI usage across 30+ models (GPT-4, Claude, Gemini, etc.)",
  "version": "1.0.8",
  "publisher": "abdulsagheer",
  "preview": false,
  "qna": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Hypothesize-Tech/ai-cost-optimizer-extension.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Snippets",
    "Visualization",
    "Other"
  ],
  "badges": [
    {
      "url": "https://img.shields.io/visual-studio-marketplace/v/abdulsagheer.cost-katana-ai-optimizer",
      "href": "https://marketplace.visualstudio.com/items?itemName=abdulsagheer.cost-katana-ai-optimizer",
      "description": "VS Marketplace Version"
    },
    {
      "url": "https://img.shields.io/visual-studio-marketplace/d/abdulsagheer.cost-katana-ai-optimizer",
      "href": "https://marketplace.visualstudio.com/items?itemName=abdulsagheer.cost-katana-ai-optimizer",
      "description": "Downloads"
    }
  ],
  "keywords": [
    "ai",
    "cost",
    "optimization",
    "cursor",
    "gpt",
    "claude",
    "gemini",
    "deepseek",
    "grok",
    "anthropic",
    "openai",
    "google",
    "machine-learning",
    "productivity",
    "analytics",
    "tracking",
    "budget",
    "monitoring",
    "chatgpt",
    "llm",
    "tokens",
    "pricing",
    "cost-savings",
    "ai-cost",
    "prompt-optimization",
    "code-generation",
    "developer-tools"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:cost-katana.connect-account",
    "onCommand:cost-katana.track-usage",
    "onCommand:cost-katana.optimize-prompt",
    "onCommand:cost-katana.show-analytics",
    "onCommand:cost-katana.setup-workspace",
    "onCommand:cost-katana.get-suggestions",
    "onCommand:cost-katana.analyze-code",
    "onCommand:cost-katana.get-model-recommendations",
    "onCommand:cost-katana.toggle-automatic-tracking"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Cost Katana AI Optimizer",
      "properties": {
        "costKatana.backendUrl": {
          "type": "string",
          "default": "https://cost-katana-backend.store/api",
          "description": "The URL of the Cost Katana backend API."
        },
        "costKatana.apiKey": {
          "type": "string",
          "description": "Your Cost Katana API Key for authentication."
        },
        "costKatana.userId": {
          "type": "string",
          "description": "Your Cost Katana User ID for authentication."
        },
        "costKatana.userEmail": {
          "type": "string",
          "description": "Your Cost Katana email address."
        },
        "costKatana.automaticTracking": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic tracking of AI interactions in Cursor."
        }
      }
    },
    "commands": [
      {
        "command": "cost-katana.connect-account",
        "title": "Cost Katana: Connect Account",
        "category": "Cost Katana"
      },
      {
        "command": "cost-katana.track-usage",
        "title": "Cost Katana: Track AI Usage",
        "category": "Cost Katana"
      },
      {
        "command": "cost-katana.optimize-prompt",
        "title": "Cost Katana: Optimize Prompt",
        "category": "Cost Katana"
      },
      {
        "command": "cost-katana.show-analytics",
        "title": "Cost Katana: Show Analytics",
        "category": "Cost Katana"
      },
      {
        "command": "cost-katana.setup-workspace",
        "title": "Cost Katana: Setup Workspace",
        "category": "Cost Katana"
      },
      {
        "command": "cost-katana.get-suggestions",
        "title": "Cost Katana: Get Suggestions",
        "category": "Cost Katana"
      },
      {
        "command": "cost-katana.analyze-code",
        "title": "Cost Katana: Analyze Code",
        "category": "Cost Katana"
      },
      {
        "command": "cost-katana.get-model-recommendations",
        "title": "Cost Katana: Get Model Recommendations",
        "category": "Cost Katana"
      },
      {
        "command": "cost-katana.toggle-automatic-tracking",
        "title": "Cost Katana: Toggle Automatic Tracking",
        "category": "Cost Katana"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src/",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.8",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^3.6.0",
    "axios": "^1.6.5",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5"
  },
  "bugs": {
    "url": "https://github.com/Hypothesize-Tech/ai-cost-optimizer-extension/issues"
  },
  "homepage": "https://cost-katana.com",
  "license": "MIT"
}
